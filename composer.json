{
    "name": "wireui/heroicons",
    "description": "The Tailwind Heroicons for laravel blade by WireUI",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "livewire icons components",
        "livewire icons",
        "wireui",
        "laravel components",
        "blade components",
        "blade heroicons"
    ],
    "authors": [
        {
            "name": "PH7-Jack",
            "email": "pedrolivertwd@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.1",
        "laravel/framework": "^9.16"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.9",
        "mockery/mockery": "^1.5",
        "orchestra/testbench": "^7.0",
        "phpunit/phpunit": "^9.5",
        "pestphp/pest": "^1.21",
        "phpstan/phpstan": "^1.8"
    },
    "scripts": {
        "test": "./vendor/bin/pest --coverage --min=100",
        "phpstan": "./vendor/bin/phpstan analyse src --level max",
        "phpcs:fix": "./vendor/bin/php-cs-fixer fix",
        "phpcs:check": "./vendor/bin/php-cs-fixer fix --dry-run --using-cache=no"
    },
    "autoload": {
        "psr-4": {
            "WireUi\\Heroicons\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "vendor/orchestra/testbench-core/laravel/app",
            "Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "WireUi\\Heroicons\\HeroiconsServiceProvider"
            ]
        },
        "aliases": []
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
